using System; 

class URI 
{

    static void Main(string[] args) 
    { 

        double A = Convert.ToDouble(Console.ReadLine());
        double B = Convert.ToDouble(Console.ReadLine());
        
        double MEDIA = (( 3.5 * A ) + ( 7.5 * B )) / 11;
        
        Console.WriteLine($"MEDIA = {MEDIA:0.00000}");
        
    }
}
