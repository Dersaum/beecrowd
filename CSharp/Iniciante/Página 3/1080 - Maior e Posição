using System;

class URI
{

    static void Main(string[] args)
    { 

        
        int valor = 0;
        int pos = 0;

        for (int i = 1; i <= 100; i++) 
        {

            int N = Int32.Parse(Console.ReadLine());

            if ( N > valor )
            {
                valor = N;
                pos = i;
            }
        }

        System.Console.WriteLine($"{valor}\n{pos}"); 
    }
}
