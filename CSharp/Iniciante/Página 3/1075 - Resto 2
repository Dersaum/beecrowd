using System; 

class URI
{

    static void Main(string[] args)
    { 

        int N = int.Parse(System.Console.ReadLine());

        for (int i = 0; i < 10000; i++) 
        {
            
            if ( i % N == 2 )
            {
                System.Console.WriteLine(i);
            }
        }
    }
}
