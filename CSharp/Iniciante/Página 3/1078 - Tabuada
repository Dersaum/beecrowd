using System; 

class URI
{

    static void Main(string[] args)
    { 

        int N = int.Parse(System.Console.ReadLine());
        int tab = 0;

        for (int i = 1; i <= 10; i++) 
        {
            tab = i * N;
            System.Console.WriteLine($"{i} x {N} = {tab}");
        }
    }
}
