using System; 

class URI
{

    static void Main(string[] args)
    { 

        int N = int.Parse(System.Console.ReadLine());
        
        for (int i = 1; i <= N; i++) 
        {
            if ( i % 2 == 0 )
            {
                double quad = Math.Pow(i, 2);

                System.Console.WriteLine($"{i}^2 = {quad}");
            }
        }
    }
}
